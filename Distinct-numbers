#include<iostream>
#include<vector>
#include<set>
using namespace std;

int main(){
    int n;
    cin >> n;
    vector<int> arr(n);
    set<int> distinctNumbers;
    for(int i = 0; i < n; i++){
        cin >> arr[i];
        distinctNumbers.insert(arr[i]);
    }
    cout << distinctNumbers.size() << endl;
}